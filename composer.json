{
	"name": "qa-data/psr7-http-message",
	"description": "HTTP message interface",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.1",
		"guzzlehttp/psr7": "^2.7"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"orisai/coding-standard": "^3.10"
	},
	"autoload": {
		"psr-4": {
			"QaData\\Psr7\\": "src/",
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
